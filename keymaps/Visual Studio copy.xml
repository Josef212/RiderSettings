<keymap version="1" name="Visual Studio copy" parent="Visual Studio">
  <action id="ChangeSplitOrientation">
    <keyboard-shortcut first-keystroke="ctrl dead_grave" second-keystroke="ctrl dead_grave" />
  </action>
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="ctrl alt multiply" />
